port { PDFViewer } from \"../../foxit-lib/PDFViewCtrl.full\";\r\n\r\nexport default {\r\n  mounted: function() {\r\n    var pdfview = this.initPDFView();\r\n    this.pdfViewer = pdfview;\r\n    this.registerPdfViewerEvents(pdfview);\r\n    // pdfview.openPDFByFile(this.file);\r\n    this.PDFFile = this.file;\r\n  },\r\n\r\n  data() {\r\n    // const a = \"11\";\r\n    return {\r\n      currentPage: 0,\r\n      pdfViewer: null,\r\n      PDFFile: null,\r\n    };\r\n  },\r\n  props: [\"control\", \"file\"],\r\n  watch: {\r\n    PDFFile() {\r\n      this.pdfViewer.openPDFByFile(this.PDFFile);\r\n    },\r\n  },\r\n  methods: {\r\n    initPDFView() {\r\n      var container = document.getElementsByClassName(\"viewer-container\");\r\n      console.debug(container);\r\n      var pdfview = new PDFViewer({\r\n        libPath: \"../../foxit-lib\",\r\n        jr: {\r\n          licenseSN: licenseSN,\r\n          licenseKey: licenseKey,\r\n        },\r\n        customs: {\r\n          ScrollWrap: class CustomScrollWrap extends ScrollWrap {\r\n            constructor(viewerRender) {\r\n              super(viewerRender);\r\n            }\r\n            getScrollHost() {\r\n              return container[0];\r\n            }\r\n            getScrollOffsetTop() {\r\n              return this.viewerRender.$ui[0].offsetTop;\r\n            }\r\n            getScrollOffsetLeft() {\r\n              return this.viewerRender.$ui[0].offsetLeft;\r\n            }\r\n            getWidth() {\r\n              return container[0].offsetWidth - 30;\r\n            }\r\n            getHeight() {\r\n              return container[0].offsetHeight;\r\n            }\r\n            getScrollTop() {\r\n              return container[0].scrollTop;\r\n            }\r\n            getScrollLeft() {\r\n              return container[0].scrollLeft;\r\n            }\r\n          },\r\n        },\r\n      });\r\n\r\n      //   fetch(\"/FoxitPDFSDKforWeb_DeveloperGuide.pdf\").then(function(response) {\r\n      //     console.debug(\"response\", response);\r\n      //     response\r\n      //       .arrayBuffer()\r\n      //       .then(function(buffer) {\r\n      //         console.debug(buffer);\r\n      //         pdfview.openPDFByFile(buffer);\r\n      //       })\r\n      //       .catch((e) => console.error(e));\r\n      //   });\r\n      pdfview.init(\"#pdf-viewer\");\r\n      // this.$parent.onInitSuccess(pdfview);\r\n\r\n      return pdfview;\r\n    },\r\n    rerenderAddonControl() {\r\n      console.log(\"RERENDER \");\r\n    },\r\n    registerPdfViewerEvents(pdfViewer /*control*/) {\r\n      const parentComponent = this.$parent;\r\n      pdfViewer.getEventEmitter().on(Events.openFileSuccess, function(doc) {\r\n        this.currentPage = 0;\r\n        // this.$parent.onOpenSuccess(doc);\r\n        // console.log(\"this parent\", parentComponent.onOpenSuccess(doc));\r\n        parentComponent.onOpenSuccess(doc);\r\n        // this.rerenderAddonControl();\r\n      });\r\n      // pdfViewer.getEventEmitter().on(Events.willCloseDocument, function() {\r\n      //   onDocumentWillClose();\r\n      // });\r\n      pdfViewer.getEventEmitter().on(Events.renderPageSuccess, function(render) {\r\n        // onRenderPageSuccess(render);\r\n        // this.$parent.onRenderPageSuccess(render);\r\n        console.log(\"Page render \", render);\r\n        parentComponent.onRenderPageSuccess(render);\r\n        // this.rerenderAddonControl();\r\n        // rerenderAddonControls(pdfViewerRef.current || {}, addonControls.current || []);\r\n      });\r\n\r\n      pdfViewer.getEventEmitter().on(Events.zoomToSuccess, function(newScale, oldScale) {\r\n        console.log(\"Scale\", oldScale, newScale);\r\n      });\r\n\r\n      pdfViewer.getEventEmitter().on(Events.pageNumberChange, function(newPageNumber) {\r\n        // setCurrentPageNo(newPageNumber - 1);\r\n        this.currentPage = newPageNumber - 1;\r\n        // onCurrentPageChange(newPageNumber - 1);\r\n      });\r\n    },\r\n\r\n    // rerenderAddonControls = function async(){\r\n    //     console.log(\"RERENDER\")\r\n    // }\r\n    //   rerenderAddonControls(pdfViewer, rerenderControls){\r\n    //   // console.log(\"rerenderAddonControls\", rerenderControls);\r\n    //   for (var control of rerenderControls) {\r\n    //     var pageRender = pdfViewer.getPDFPageRender(control.pageNo);\r\n    //     var parentElement = getParentElementByPageNo(control.pageNo);\r\n    //     if (parentElement) {\r\n    //       var controlRect = await PdfHelper.toDeviceRect(\r\n    //         pageRender,\r\n    //         control.pageRect,\r\n    //         controls.scale,\r\n    //       );\r\n    //       // console.log(\"device rect\", controlRect);\r\n    //       var currentPageScale = pageRender.getScale();\r\n    //       var scale = currentPageScale - control.scale;\r\n    //       // console.log(currentPageScale, control.scale, scale);\r\n    //       var div = document.getElementById(control.id);\r\n    //       // console.log(div);\r\n    //       if (div) {\r\n    //         const offset = 0;\r\n    //         div.style.top = controlRect.top - offset + \"px\";\r\n    //         div.style.left = Math.abs(controlRect.left - offset) + \"px\";\r\n    //         div.style.width = controlRect.right - controlRect.left + \"px\";\r\n    //         div.style.height = controlRect.bottom - controlRect.top + \"px\";\r\n    //       }\r\n    //     }\r\n    //   }\r\n    // }\r\n  },\r\n};\r\n</script>\r\n<style scoped src=\"./PDFView.css\"></style>\r\n"],"sourceRoot":"src/components/WVS"}]}